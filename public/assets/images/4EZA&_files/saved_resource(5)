jQuery(function(){
		var coverflow;

		if ( jQuery(".coverflow").length > 0 ) {
			coverflow = jQuery(".coverflow").flipster();
		}
});

//Ajoute un attribut commun à un groupe de div exemple (data-group="accordion_1")
function addAttr(elt, classe, attr, data) {
	if (elt && attr && data) {
		if( !jQuery(elt).attr(attr) ) {
			jQuery(elt).attr(attr, data);
			jQuery(elt).next(classe).each( function() {
				addAttr( jQuery(this), classe, attr, data);
			});
		}
	}
}
//Parcours tous les groupes d'une même classe pour leurs ajouter un attribut commun
function setAttrSiblings(classe, type) {
	if (classe) {
		var elt = jQuery('body').find(classe).each( function( index ) {
			addAttr( jQuery(this), classe, 'data-group', type+'_'+index);
		});

		var attr = jQuery(classe).map(function() {
			return jQuery(this).attr('data-group');
		}).get();

		return jQuery.unique(attr);
	}
}

